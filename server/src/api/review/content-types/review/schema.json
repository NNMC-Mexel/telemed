{
  "kind": "collectionType",
  "collectionName": "reviews",
  "info": {
    "singularName": "review",
    "pluralName": "reviews",
    "displayName": "Review"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "patient": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "reviews"
    },
    "doctor": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::doctor.doctor",
      "inversedBy": "reviews"
    },
    "appointment": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::appointment.appointment"
    },
    "rating": {
      "type": "integer",
      "required": true,
      "min": 1,
      "max": 10
    },
    "text": {
      "type": "text"
    },
    "isPublished": {
      "type": "boolean",
      "default": false
    }
  }
}
