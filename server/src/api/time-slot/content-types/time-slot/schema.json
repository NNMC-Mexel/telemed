{
  "kind": "collectionType",
  "collectionName": "time_slots",
  "info": {
    "singularName": "time-slot",
    "pluralName": "time-slots",
    "displayName": "TimeSlot"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "doctor": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::doctor.doctor",
      "inversedBy": "time_slots"
    },
    "date": {
      "type": "date",
      "required": true
    },
    "time": {
      "type": "string",
      "required": true
    },
    "isBooked": {
      "type": "boolean",
      "default": false
    },
    "appointment": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::appointment.appointment"
    }
  }
}
