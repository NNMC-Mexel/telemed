{
  "kind": "collectionType",
  "collectionName": "appointments",
  "info": {
    "singularName": "appointment",
    "pluralName": "appointments",
    "displayName": "Appointment"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "patient": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "appointments"
    },
    "doctor": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::doctor.doctor",
      "inversedBy": "appointments"
    },
    "dateTime": {
      "type": "datetime"
    },
    "statuse": {
      "type": "enumeration",
      "default": "pending",
      "enum": [
        "pending",
        "confirmed",
        "in_progress",
        "completed",
        "cancelled"
      ]
    },
    "type": {
      "type": "enumeration",
      "default": "video",
      "enum": [
        "video",
        "chat"
      ]
    },
    "price": {
      "type": "integer",
      "required": true
    },
    "roomId": {
      "type": "string"
    },
    "rating": {
      "type": "integer",
      "min": 1,
      "max": 10
    },
    "review": {
      "type": "text"
    },
    "paymentId": {
      "type": "string"
    },
    "paymentStatus": {
      "type": "enumeration",
      "default": "pending",
      "enum": [
        "pending",
        "paid",
        "refunded",
        "failed"
      ]
    },
    "medical_documents": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::medical-document.medical-document",
      "mappedBy": "appointment"
    }
  }
}
